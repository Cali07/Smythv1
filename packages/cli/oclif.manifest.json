{
  "commands": {
    "agent": {
      "aliases": [],
      "args": {
        "path": {
          "description": "Path to the agent file (.smyth)",
          "name": "path",
          "required": false
        }
      },
      "description": "Run .smyth agent with various execution modes",
      "examples": [
        "<%= config.bin %> <%= command.id %> ./myagent.smyth --chat",
        "<%= config.bin %> <%= command.id %> ./myagent.smyth --prompt \"What is the weather in Tokyo?\"",
        "<%= config.bin %> <%= command.id %> ./myagent.smyth --skill ask question=\"who are you\"",
        "<%= config.bin %> <%= command.id %> ./myagent.smyth --mcp sse"
      ],
      "flags": {
        "help": {
          "char": "h",
          "description": "Show CLI help.",
          "name": "help",
          "allowNo": false,
          "type": "boolean"
        },
        "chat": {
          "char": "c",
          "description": "Start chat interface\n\n ",
          "name": "chat",
          "hasDynamicHelp": false,
          "helpValue": "[model-name]",
          "multiple": false,
          "type": "option"
        },
        "models": {
          "char": "M",
          "description": "A list of models to use in a json file or directory of json files\n\n ",
          "name": "models",
          "hasDynamicHelp": false,
          "helpValue": "<models> [options]",
          "multiple": false,
          "type": "option"
        },
        "skill": {
          "char": "s",
          "description": "Execute an Agent skill, you can pass input parameters as key=\"value\" pairs\n\n ",
          "name": "skill",
          "hasDynamicHelp": false,
          "helpValue": "<skill> [key1=\"value1\" ...]",
          "multiple": true,
          "type": "option"
        },
        "prompt": {
          "char": "p",
          "description": "Query agent with a prompt, by default it will use the agent model or default to gpt-4o, you can force a model by passing it as a second argument after the prompt\n\n ",
          "helpLabel": "--prompt",
          "name": "prompt",
          "hasDynamicHelp": false,
          "helpValue": "<prompt> [model]",
          "multiple": true,
          "type": "option"
        },
        "mcp": {
          "description": "Start MCP server\n\n ",
          "name": "mcp",
          "hasDynamicHelp": false,
          "helpValue": "[server-type] [port]",
          "multiple": true,
          "type": "option"
        },
        "vault": {
          "char": "v",
          "description": "Provide a vault path to use for the agent\nExample: sre ./myagent.smyth --chat --vault ./myvault.json\n\n ",
          "helpLabel": "--vault",
          "name": "vault",
          "hasDynamicHelp": false,
          "helpValue": "<vault-path>",
          "multiple": false,
          "type": "option"
        }
      },
      "hasDynamicHelp": false,
      "hiddenAliases": [],
      "id": "agent",
      "pluginAlias": "@smythos/cli",
      "pluginName": "@smythos/cli",
      "pluginType": "core",
      "strict": true,
      "usage": "<path> [options]",
      "enableJsonFlag": false,
      "isESM": true,
      "relativePath": [
        "dist",
        "commands",
        "agent.js"
      ]
    },
