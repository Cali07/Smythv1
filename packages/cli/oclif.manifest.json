        "<%= config.bin %> <%= command.id %>",
        "<%= config.bin %> <%= command.id %> --check",
        "<%= config.bin %> <%= command.id %> --force"
      ],
      "flags": {
        "help": {
          "char": "h",
          "description": "Show CLI help.",
          "name": "help",
          "allowNo": false,
          "type": "boolean"
        },
        "check": {
          "char": "c",
