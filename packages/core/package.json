{
    "name": "smyth-runtime",
    "version": "0.0.0",
    "description": "Smyth Runtime Environment",
    "main": "rollup.config.js",
    "type": "module",
    "scripts": {
        "test": "cross-env TEST_ENV=true vitest --reporter verbose",
        "cli-test": "vitest --reporter verbose run cli.test.ts",
        "build": "rollup -c",
        "doc:graphgen": "npx depcruise src --config .dependency-cruiser.cjs --output-type dot > ./doc/dep-graph.dot && dot -Tpng ./doc/dep-graph.dot -o ./doc/dep-graph.png",
        "knip": "knip"
    },
    "author": "Alaa-eddine KADDOURI",
    "devDependencies": {
        "@rollup/plugin-alias": "^5.1.0",
        "@rollup/plugin-commonjs": "^26.0.1",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/pluginutils": "^5.1.0",
        "@types/node": "^20.14.2",
        "@vitest/coverage-v8": "^1.6.0",
        "cross-env": "^7.0.3",
        "dependency-cruiser": "^16.3.3",
        "dot": "^1.1.3",
        "knip": "^5.23.1",
        "rollup-plugin-copy": "^3.5.0",
        "rollup-plugin-esbuild": "^6.1.1",
        "rollup-plugin-sourcemaps": "^0.6.3",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript-paths": "^1.5.0",
        "rollup-plugin-typescript2": "^0.36.0",
        "tsc": "^2.0.4",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.4.5",
        "vite-tsconfig-paths": "^4.3.2",
        "vitest": "^1.6.0"
