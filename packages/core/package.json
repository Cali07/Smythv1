{
    "name": "smyth-runtime",
    "version": "1.3.9",
    "description": "Smyth Runtime Environment",
    "main": "dist/index.dev.js",
    "types": "dist/types/index.d.ts",
    "files": [
        "dist"
    ],
    "type": "module",
    "scripts": {
        "test": "cross-env TEST_ENV=true vitest --reporter verbose",
        "cli-test": "vitest --reporter verbose run cli.test.ts",
        "build:dev": "cross-env BUILD=dev rollup -c && tsc --emitDeclarationOnly --declaration --outDir dist/types -p tsconfig.dts.json && npm link",
        "build:prod": "cross-env BUILD=prod rollup -c",
        "doc:graphgen": "npx depcruise src --config ./doc/.dep-minimal.json --output-type dot > ./doc/dep-graph.dot && dot -Tpng ./doc/dep-graph.dot -o ./doc/dep-graph.png",
        "doc:hgen": "node ./scripts/hgen.js > ./doc/hierarchy.txt",
        "knip": "knip",
        "build:web": "npm run build:dev && cp -r dist/*.js dist/*.js.map tests/build/web/sre/",
        "start:web": "node ./tests/build/web/index.js --model claude-3-5-haiku-latest",
        "build:aws:dev:cjs": "cross-env BUILD=dev rollup -c distributions/AWS/rollup.config.js",
        "build:aws:dev:exe": "pkg ./distributions/AWS/dist/aws.dev.cjs --targets node18-win32-x64,node18-macos-x64,node18-linux-x64 --out-path ./distributions/AWS/dist",
        "build:aws:dev": "npm run build:aws:dev:cjs && npm run build:aws:dev:exe",
        "build:aws:prod:cjs": "cross-env BUILD=prod rollup -c distributions/AWS/rollup.config.js",
        "build:aws:prod:exe": "pkg ./distributions/AWS/dist/aws.prod.cjs --targets node18-win32-x64,node18-macos-x64,node18-linux-x64 --out-path ./distributions/AWS/dist",
        "build:aws:prod": "npm run build:aws:prod:cjs && npm run build:aws:prod:exe && mv distributions/AWS/dist/aws.prod-linux distributions/AWS/dist/smyth-runtime",
        "build:cli:debug": "cross-env BUILD=debug rollup -c distributions/cli/rollup.config.js",
        "build:cli:cjs": "cross-env BUILD=prod FORMAT=cjs rollup -c distributions/cli/rollup.config.js",
        "build:cli:exe": "pkg ./distributions/cli/dist/smyth-runtime.cjs --targets node18-win32-x64,node18-macos-x64,node18-linux-x64 --out-path ./distributions/cli/dist",
        "build:cli": "npm run build:cli:cjs && npm run build:cli:exe",
        "build:agent-builder:dev:es": "cross-env BUILD=dev rollup -c distributions/agent-builder/rollup.config.js",
        "build:agent-builder:dev:esbundle": "cross-env BUILD=dev FORMAT=esbundle rollup -c distributions/agent-builder/rollup.config.js",
        "build:agent-builder:dev:cjs": "cross-env BUILD=dev FORMAT=cjs rollup -c distributions/agent-builder/rollup.config.js",
        "build:agent-builder:dev:exe": "pkg ./distributions/agent-builder/dist/agent-builder.dev.js --targets node18-win32-x64,node18-macos-x64,node18-linux-x64 --out-path ./distributions/agent-builder/dist",
        "build:agent-builder:dev": "npm run build:agent-builder:dev:cjs && npm run build:agent-builder:dev:exe",
        "build:agent-server:dev:es": "cross-env BUILD=dev rollup -c distributions/agent-server/rollup.config.js",
        "build:agent-server:dev:esbundle": "cross-env BUILD=dev FORMAT=esbundle rollup -c distributions/agent-server/rollup.config.js",
        "build:agent-server:dev:cjs": "cross-env BUILD=dev FORMAT=cjs rollup -c distributions/agent-server/rollup.config.js",
        "build:agent-server:dev:exe": "pkg ./distributions/agent-server/dist/agent-server.dev.js --targets node18-win32-x64,node18-macos-x64,node18-linux-x64 --out-path ./distributions/agent-builder/dist",
        "build:agent-server:dev": "npm run build:agent-server:dev:cjs && npm run build:agent-server:dev:exe",
        "build:agent-debugger:dev:es": "cross-env BUILD=dev rollup -c distributions/agent-debugger/rollup.config.js",
        "build:agent-debugger:dev:esbundle": "cross-env BUILD=dev FORMAT=esbundle rollup -c distributions/agent-debugger/rollup.config.js",
        "build:agent-debugger:dev:cjs": "cross-env BUILD=dev FORMAT=cjs rollup -c distributions/agent-debugger/rollup.config.js",
        "build:agent-debugger:dev:exe": "pkg ./distributions/agent-debugger/dist/agent-debugger.dev.js --targets node18-win32-x64,node18-macos-x64,node18-linux-x64 --out-path ./distributions/agent-builder/dist",
        "build:agent-debugger:dev": "npm run build:agent-debugger:dev:cjs && npm run build:agent-debugger:dev:exe",
        "build:embodiments-server:dev:es": "cross-env BUILD=dev rollup -c distributions/embodiments-server/rollup.config.js",
        "build:embodiments-server:dev:esbundle": "cross-env BUILD=dev FORMAT=esbundle rollup -c distributions/embodiments-server/rollup.config.js",
        "build:embodiments-server:dev:cjs": "cross-env BUILD=dev FORMAT=cjs rollup -c distributions/embodiments-server/rollup.config.js",
        "build:embodiments-server:dev:exe": "pkg ./distributions/embodiments-server/dist/embodiments-server.dev.js --targets node18-win32-x64,node18-macos-x64,node18-linux-x64 --out-path ./distributions/agent-builder/dist",
        "build:embodiments-server:dev": "npm run build:embodiments-server:dev:cjs && npm run build:embodiments-server:dev:exe",
        "start:embodiments-server": "npm run build:embodiments-server:dev:es && node ./distributions/embodiments-server/dist/embodiments-server.dev.js"
    },
    "author": "Alaa-eddine KADDOURI",
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@rollup/plugin-alias": "^5.1.0",
        "@rollup/plugin-commonjs": "^26.0.1",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/pluginutils": "^5.1.0",
        "@types/cookie-parser": "^1.4.7",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.18.0",
        "@types/lodash": "^4.17.10",
        "@types/node": "^20.14.2",
        "@vitest/coverage-v8": "^2.0.5",
        "blessed": "^0.1.81",
        "blessed-contrib": "^1.0.11",
        "chalk": "^5.3.0",
        "cross-env": "^7.0.3",
        "dependency-cruiser": "^16.3.3",
        "keypress": "^0.2.1",
        "knip": "^5.23.1",
        "lz-string": "^1.5.0",
        "neo-blessed": "^0.2.0",
        "node-global-key-listener": "^0.3.0",
        "nyc": "^17.0.0",
        "ora": "^8.1.0",
        "pkg": "^5.8.1",
        "rollup-plugin-copy": "^3.5.0",
        "rollup-plugin-esbuild": "^6.1.1",
        "rollup-plugin-sourcemaps": "^0.6.3",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript-paths": "^1.5.0",
        "rollup-plugin-typescript2": "^0.36.0",
        "source-map-support": "^0.5.21",
        "terminal-kit": "^3.1.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.4.5",
        "vite-tsconfig-paths": "^4.3.2",
        "vitest": "^2.0.5"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.29.0",
        "@apidevtools/json-schema-ref-parser": "^11.6.4",
        "@aws-sdk/client-bedrock-runtime": "^3.651.1",
        "@aws-sdk/client-lambda": "^3.699.0",
        "@aws-sdk/client-s3": "^3.529.0",
        "@aws-sdk/client-secrets-manager": "^3.649.0",
        "@google-cloud/vertexai": "^1.7.0",
        "@google/generative-ai": "^0.14.1",
        "@huggingface/inference": "^2.8.0",
        "@langchain/pinecone": "^0.0.8",
        "@pinecone-database/pinecone": "^3.0.0",
        "aws-sdk": "^2.1691.0",
        "axios": "^1.7.2",
        "chokidar": "^4.0.1",
        "commander": "^11.1.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dayjs": "^1.11.11",
        "dotenv": "^16.4.5",
        "eventsource": "^3.0.2",
        "express": "^4.19.2",
        "express-async-handler": "^1.2.0",
        "express-session": "^1.18.1",
        "file-type": "^19.0.0",
        "gpt-tokenizer": "^2.2.1",
        "groq-sdk": "^0.6.1",
        "image-size": "^1.1.1",
        "ioredis": "^5.4.1",
        "isbinaryfile": "^5.0.2",
        "joi": "^17.13.1",
        "js-yaml": "^4.1.0",
        "jsonrepair": "^3.8.0",
        "jsonwebtoken": "^9.0.2",
        "jwks-rsa": "^3.1.0",
        "langchain": "^0.2.10",
        "lodash": "^4.17.21",
        "memorystore": "^1.6.7",
        "mime": "^4.0.3",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.11.3",
        "oauth-1.0a": "^2.2.6",
        "openai": "^4.52.2",
        "p-limit": "^6.1.0",
        "qs": "^6.13.0",
        "rate-limiter-flexible": "^5.0.3",
        "socks-proxy-agent": "^8.0.4",
        "source-map-support": "^0.5.21",
        "uuid": "^10.0.0",
        "winston": "^3.13.0",
        "winston-transport": "^4.7.0",
        "xxhashjs": "^0.2.2"
    }
}
