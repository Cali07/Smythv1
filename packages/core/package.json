{
    "name": "smyth-runtime",
    "version": "0.0.0",
    "description": "Smyth Runtime Environment",
    "main": "rollup.config.js",
    "type": "module",
    "scripts": {
        "test": "cross-env TEST_ENV=true vitest --reporter verbose",
        "cli-test": "vitest --reporter verbose run cli.test.ts",
        "build:dev": "cross-env BUILD=dev rollup -c",
        "build:prod": "cross-env BUILD=prod rollup -c",
        "doc:graphgen": "npx depcruise src --config ./doc/.dep-minimal.json --output-type dot > ./doc/dep-graph.dot && dot -Tpng ./doc/dep-graph.dot -o ./doc/dep-graph.png",
        "doc:hgen": "node ./scripts/hgen.js > ./doc/hierarchy.txt",
        "knip": "knip",
        "build:web": "npm run build:dev && cp -r dist/*.js dist/*.js.map tests/build/web/sre/",
        "start:web": "node ./tests/build/web/index.js --model claude-3-5-sonnet-20240620",
        "build:aws:dev": "cross-env BUILD=dev rollup -c distributions/AWS/rollup.config.js",
        "build:aws:prod": "cross-env BUILD=prod rollup -c distributions/AWS/rollup.config.js"
    },
    "author": "Alaa-eddine KADDOURI",
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@rollup/plugin-alias": "^5.1.0",
        "@rollup/plugin-commonjs": "^26.0.1",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/pluginutils": "^5.1.0",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.18.0",
        "@types/node": "^20.14.2",
        "@vitest/coverage-v8": "^2.0.5",
        "blessed": "^0.1.81",
        "blessed-contrib": "^1.0.11",
        "chalk": "^5.3.0",
        "cross-env": "^7.0.3",
        "dependency-cruiser": "^16.3.3",
        "express": "^4.19.2",
        "express-session": "^1.18.0",
        "keypress": "^0.2.1",
        "knip": "^5.23.1",
        "neo-blessed": "^0.2.0",
        "node-global-key-listener": "^0.3.0",
        "nyc": "^17.0.0",
        "ora": "^8.1.0",
        "rollup-plugin-copy": "^3.5.0",
        "rollup-plugin-esbuild": "^6.1.1",
        "rollup-plugin-sourcemaps": "^0.6.3",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript-paths": "^1.5.0",
        "rollup-plugin-typescript2": "^0.36.0",
        "source-map-support": "^0.5.21",
        "terminal-kit": "^3.1.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.4.5",
        "vite-tsconfig-paths": "^4.3.2",
        "vitest": "^2.0.5"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.25.0",
        "@apidevtools/json-schema-ref-parser": "^11.6.4",
        "@apidevtools/swagger-parser": "^10.1.0",
        "@aws-sdk/client-bedrock-runtime": "^3.651.1",
        "@aws-sdk/client-s3": "^3.529.0",
        "@aws-sdk/client-secrets-manager": "^3.649.0",
        "@google-cloud/vertexai": "^1.7.0",
        "@google/generative-ai": "^0.14.1",
        "@huggingface/inference": "^2.8.0",
        "@langchain/pinecone": "^0.0.8",
        "@node-rs/xxhash": "^1.7.3",
        "@pinecone-database/pinecone": "^3.0.0",
        "axios": "^1.7.2",
        "dayjs": "^1.11.11",
        "dotenv": "^16.4.5",
        "file-type": "^19.0.0",
        "gpt-tokenizer": "^2.2.1",
        "groq-sdk": "^0.6.1",
        "image-size": "^1.1.1",
        "ioredis": "^5.4.1",
        "isbinaryfile": "^5.0.2",
        "joi": "^17.13.1",
        "js-yaml": "^4.1.0",
        "jsonrepair": "^3.8.0",
        "langchain": "^0.2.10",
        "memorystore": "^1.6.7",
        "mime": "^4.0.3",
        "oauth-1.0a": "^2.2.6",
        "openai": "^4.52.2",
        "p-limit": "^6.1.0",
        "qs": "^6.13.0",
        "winston": "^3.13.0",
        "winston-transport": "^4.7.0"
    }
}
