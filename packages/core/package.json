        "dotenv": "^16.4.5",
        "file-type": "^19.0.0",
        "gpt-tokenizer": "^2.1.2",
        "ioredis": "^5.4.1",
        "isbinaryfile": "^5.0.2",
        "joi": "^17.13.1",
        "js-yaml": "^4.1.0",
        "jsonrepair": "^3.8.0",
        "mime": "^4.0.3",
        "openai": "^4.52.2",
