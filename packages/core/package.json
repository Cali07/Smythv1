{
    "name": "smyth-runtime",
    "version": "1.4.4",
    "description": "Smyth Runtime Environment",
    "main": "dist/index.dev.js",
    "types": "dist/types/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.dev.js",
            "types": "./dist/types/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "type": "module",
    "scripts": {
        "test": "cross-env TEST_ENV=true vitest --reporter verbose",
        "cli-test": "vitest --reporter verbose run cli.test.ts",
        "gen:barrel": "ctix build",
        "sdk:gen": "node scripts/sdk/generate-components.js",
        "build:dev": "cross-env BUILD=dev rollup -c && tsc --emitDeclarationOnly --declaration --outDir dist/types -p tsconfig.dts.json && npm link",
        "build:prod": "cross-env BUILD=prod rollup -c",
        "doc:graphgen": "npx depcruise src --config ./doc/.dep-minimal.json --output-type dot > ./doc/dep-graph.dot && dot -Tpng ./doc/dep-graph.dot -o ./doc/dep-graph.png",
        "doc:hgen": "node ./scripts/hgen.js > ./doc/hierarchy.txt",
        "knip": "knip",
        "build:web": "npm run build:dev && cp -r dist/*.js dist/*.js.map tests/build/web/sre/",
        "start:web": "node ./tests/build/web/index.js --model claude-3-5-haiku-latest"
    },
    "author": "Alaa-eddine KADDOURI",
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/pluginutils": "^5.1.0",
        "@types/cookie-parser": "^1.4.7",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.18.0",
        "@types/lodash": "^4.17.10",
        "@types/node": "^20.14.2",
        "@vitest/coverage-v8": "^3.1.4",
        "cross-env": "^7.0.3",
        "ctix": "^2.7.1",
        "dependency-cruiser": "^16.3.3",
        "esbuild": "^0.25.0",
        "knip": "^5.23.1",
        "nyc": "^17.0.0",
        "pkg": "^5.8.1",
        "rollup-plugin-copy": "^3.5.0",
        "rollup-plugin-esbuild": "^6.1.1",
        "rollup-plugin-sourcemaps": "^0.6.3",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript-paths": "^1.5.0",
        "rollup-plugin-typescript2": "^0.36.0",
        "source-map-support": "^0.5.21",
        "typescript": "^5.4.5",
        "vite-tsconfig-paths": "^4.3.2",
        "vitest": "^3.1.4"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.39.0",
        "@apidevtools/json-schema-ref-parser": "^11.6.4",
        "@aws-sdk/client-bedrock-runtime": "^3.651.1",
        "@aws-sdk/client-iam": "^3.750.0",
        "@aws-sdk/client-lambda": "^3.699.0",
        "@aws-sdk/client-s3": "^3.529.0",
        "@aws-sdk/client-secrets-manager": "^3.649.0",
        "@google-cloud/vertexai": "^1.7.0",
        "@google/generative-ai": "^0.14.1",
        "@huggingface/inference": "^2.8.0",
        "@modelcontextprotocol/sdk": "^1.10.1",
        "@pinecone-database/pinecone": "^3.0.0",
        "@runware/sdk-js": "^1.1.36",
        "aws-sdk": "^2.1692.0",
        "axios": "^1.7.2",
        "chokidar": "^4.0.1",
        "commander": "^11.1.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dayjs": "^1.11.11",
        "dotenv": "^16.4.5",
        "eventsource": "^3.0.2",
        "express": "^4.19.2",
        "express-async-handler": "^1.2.0",
        "express-session": "^1.18.1",
        "file-type": "^19.0.0",
        "gpt-tokenizer": "^2.2.1",
        "groq-sdk": "^0.6.1",
        "image-size": "^1.1.1",
        "ioredis": "^5.4.1",
        "isbinaryfile": "^5.0.2",
        "joi": "^17.13.1",
        "js-yaml": "^4.1.0",
        "jsonrepair": "^3.8.0",
        "jsonwebtoken": "^9.0.2",
        "jwks-rsa": "^3.1.0",
        "langchain": "^0.2.10",
        "lodash": "^4.17.21",
        "memorystore": "^1.6.7",
        "mime": "^4.0.3",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.11.3",
        "oauth-1.0a": "^2.2.6",
        "openai": "^4.52.2",
