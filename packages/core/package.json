        "rate-limiter-flexible": "^5.0.3",
        "socket.io-client": "^4.8.1",
        "socks-proxy-agent": "^8.0.4",
        "source-map-support": "^0.5.21",
        "uuid": "^10.0.0",
        "winston": "^3.13.0",
